# [START gae_quickstart_yaml]
runtime: nodejs
env: flex

env_variables:
  DB_HOST: localhost
  DB_USER: user
  DB_PASS: password
  DB_NAME: name

  DB_SOCKET_PATH: mysql:unix_socket=/cloudsql
  CLOUD_SQL_CONNECTION_NAME: instance-connection-name

beta_settings:
  cloud_sql_instances: instance-connection-name
# [END gae_quickstart_yaml]
